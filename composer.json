{
    "name": "yo2uno/polyfill-stringmb",
    "type": "library",
    "description": "Symfony polyfill for the stringmb extension",
    "keywords": ["polyfill", "shim", "compatibility", "portable", "stringmb"],
    "homepage": "https://blckswan.no",
    "license": "MIT",
    "authors": [
        {
            "name": "Anders Pippi",
            "email": "p@tchwork.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://blckswan.no/contributors"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "provide": {
        "ext-stringmb": "*"
    },
    "autoload": {
        "psr-4": { "Symfony\\Polyfill\\stringmb\\": "" },
        "files": [ "bootstrap.php" ]
    },
    "suggest": {
        "ext-stringmb": "For best performance"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-main": "1.27-dev"
        },
        "thanks": {
            "name": "symfony/polyfill",
            "url": "https://github.com/symfony/polyfill"
        }
    }
}
